				<xs:complexType>
					<xs:choice maxOccurs="6">
						<xs:element name="given" type="DICOMNAME" minOccurs="0" maxOccurs="4"/>
						<xs:element name="family" type="DICOMNAME" minOccurs="1" maxOccurs="2"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
