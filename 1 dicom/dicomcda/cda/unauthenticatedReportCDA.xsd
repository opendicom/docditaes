	</xs:complexType>

	<xs:complexType name="ATNDassignedEntity">
		<xs:sequence>
			<xs:element name="assignedEntity">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="id">
							<xs:complexType>
								<xs:attribute name="extension" type="DICOMPNosp">
